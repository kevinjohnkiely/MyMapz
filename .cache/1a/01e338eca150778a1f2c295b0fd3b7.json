\r\n\r\n$gradient: linear-gradient(to right bottom, $color-main, $color-sec);\r\n\r\n// CSS resets\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n}\r\n\r\n*,\r\n*::before,\r\n*::after {\r\n  box-sizing: inherit;\r\n}\r\n\r\nbody {\r\n  font-family: \"Roboto\", sans-serif;\r\n  font-weight: 400;\r\n  line-height: 1.6;\r\n  background-image: $gradient;\r\n  background-size: cover;\r\n  background-repeat: no-repeat;\r\n  min-height: 100vh;\r\n}\r\n\r\n.page-container {\r\n  background-color: whitesmoke;\r\n  //   max-width: 90%;\r\n  max-width: 65rem;\r\n  min-height: 75vh;\r\n  margin: 2vw auto;\r\n  padding: 2rem;\r\n  border-radius: 1.5rem;\r\n\r\n  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\r\n\r\n  display: grid;\r\n  //   grid-template-rows: 10rem minmax(100rem, auto);\r\n  grid-template-columns: 1fr 3fr;\r\n}\r\n\r\n.logo {\r\n  width: 100%;\r\n}\r\n\r\n.main-content {\r\n  // background-color: red;\r\n  padding-left: 1rem;\r\n}\r\n\r\n.search-form input[type=\"text\"] {\r\n  width: 100%;\r\n  padding: 12px 20px;\r\n  margin: 8px 0;\r\n  display: inline-block;\r\n  border: 1px solid #ccc;\r\n  border-radius: 4px;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.search-form input[type=\"submit\"] {\r\n  width: 100%;\r\n  background-color: $color-main;\r\n  color: white;\r\n  padding: 14px 20px;\r\n  margin: 8px 0;\r\n  border: none;\r\n  border-radius: 4px;\r\n  cursor: pointer;\r\n  box-sizing: border-box;\r\n}\r\n\r\n.search-form input[type=\"submit\"]:hover {\r\n  background-color: $color-sec;\r\n}\r\n\r\n.search-results {\r\n  column-gap: 1rem;\r\n  row-gap: 1rem;\r\n  margin: 10px;\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr 1fr 1fr;\r\n}\r\n\r\n.search-results-holder {\r\n  display: grid;\r\n  grid-template-columns: 1fr 2fr;\r\n  background-color: $color-sec;\r\n  line-height: 1rem;\r\n  padding: 5px;\r\n  background-color: white;\r\n  border-radius: 5px;\r\n  border: 2px dashed $color-main;\r\n}\r\n\r\n.search-results-holder:hover, .search-results-holder-active {\r\n  background-color: whitesmoke;\r\n  border: 2px dashed $color-sec;\r\n}\r\n\r\n.search-results-flag-round img {\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 50%;\r\n}\r\n\r\n.search-results-country-name {\r\n  line-height: 40px;\r\n}\r\n\r\n.result-link {\r\n  text-decoration: none;\r\n  color: $color-sec;\r\n}\r\n\r\n\r\n.country-container {\r\n  \r\n  height: 95%;\r\n  // opacity: 0.3;\r\n  padding: 1rem;\r\n  \r\n}\r\n\r\n.country-container-header {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\n.country-container-content {\r\n  display: grid;\r\n  grid-template-columns: 1fr 1fr;\r\n  column-gap: 1rem;\r\n}\r\n\r\n.country-message, .country-error {\r\n  text-align: center;\r\n  margin-top:2rem;\r\n  font-style: italic;\r\n  font-weight: 500;\r\n}\r\n\r\n.country-details ul {\r\n  list-style-type: none;\r\n}\r\n\r\n.country-flag img {\r\n  border: 1rem solid white;\r\n}\r\n\r\n.error-icon img{\r\n  max-width: 100px;\r\n}\r\n\r\n.btn-fave {\r\n  background-color: $color-sec;\r\n  padding: 1rem;\r\n  border-radius: 2rem;\r\n  border: none;\r\n  text-transform: uppercase;\r\n  color: #fff;\r\n  cursor: pointer;\r\n}\r\n\r\n.fave-list {\r\n  margin-left: 1rem;\r\n  list-style-image: url('../img/list.png');\r\n}\r\n\r\n.fave-list li a {\r\n  line-height: 13px;\r\n  text-decoration: none;\r\n  color: $color-sec\r\n}\r\n\r\n.fave-list li a:hover {\r\n  color: $color-main;\r\n}","..\\src\\sass\\_loader.scss":".loader,\r\n.loader:after {\r\n  border-radius: 50%;\r\n  width: 10em;\r\n  height: 10em;\r\n}\r\n.loader {\r\n  margin: 10px auto;\r\n  font-size: 10px;\r\n  position: relative;\r\n  text-indent: -9999em;\r\n  border-top: 1.1em solid rgba(255, 255, 255, 0.2);\r\n  border-right: 1.1em solid rgba(255, 255, 255, 0.2);\r\n  border-bottom: 1.1em solid rgba(255, 255, 255, 0.2);\r\n  border-left: 1.1em solid $color-main;\r\n  -webkit-transform: translateZ(0);\r\n  -ms-transform: translateZ(0);\r\n  transform: translateZ(0);\r\n  -webkit-animation: load8 1.1s infinite linear;\r\n  animation: load8 1.1s infinite linear;\r\n}\r\n@-webkit-keyframes load8 {\r\n  0% {\r\n    -webkit-transform: rotate(0deg);\r\n    transform: rotate(0deg);\r\n  }\r\n  100% {\r\n    -webkit-transform: rotate(360deg);\r\n    transform: rotate(360deg);\r\n  }\r\n}\r\n@keyframes load8 {\r\n  0% {\r\n    -webkit-transform: rotate(0deg);\r\n    transform: rotate(0deg);\r\n  }\r\n  100% {\r\n    -webkit-transform: rotate(360deg);\r\n    transform: rotate(360deg);\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"f0db43fd4a4a1e26bf710f439b5f3fa0","cacheData":{"env":{}}}